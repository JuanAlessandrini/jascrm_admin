{% extends 'dashboard/dashboard_base.html.twig' %}

{% block title %}EntityTypeDoc index{% endblock %}

{% block content %}

<h5>{{reporte.name}}</h5>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reporte in reportes %}
            <tr>
                <td>{{ reporte.id }}</td>
                <td>{{ reporte.name }}</td>
                <td>
                    <a href="{{ path('app_reporte_show', {'id': reporte.id}) }}">show</a>
                    <a href="{{ path('app_reporte_edit', {'id': reporte.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_reporte_new') }}">Create new</a>
{% endblock %}
