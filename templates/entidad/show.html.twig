{% extends 'base.html.twig' %}

{% block title %}Entidad{% endblock %}

{% block body %}
    <h1>Entidad</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entidad.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entidad.name }}</td>
            </tr>
            <tr>
                <th>Has_impuestos</th>
                <td>{{ entidad.hasImpuestos ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Has_perception</th>
                <td>{{ entidad.hasPerception ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Has_retencion</th>
                <td>{{ entidad.hasRetencion ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_entidad_index') }}">back to list</a>

    <a href="{{ path('app_entidad_edit', {'id': entidad.id}) }}">edit</a>

    {{ include('entidad/_delete_form.html.twig') }}
{% endblock %}
