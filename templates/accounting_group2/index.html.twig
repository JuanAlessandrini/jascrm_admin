{% extends 'dashboard/dashboard_base.html.twig' %}

{% block title %}AccountingGroup2 index{% endblock %}

{% block content %}
    <h1>SubGrupo Cuentas</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for accounting_group2 in accounting_group2s %}
            <tr>
                <td>{{ accounting_group2.id }}</td>
                <td>{{ accounting_group2.name }}</td>
                <td>
                    
                    <a href="{{ path('app_accounting_group2_edit', {'id': accounting_group2.id}) }}"><i class='bx bxs-edit-alt'></i></a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a class="btn btn-primary" href="{{ path('app_accounting_group2_new') }}">Nueva</a>
{% endblock %}
