{% extends 'dashboard/dashboard_base.html.twig' %}

{% block title %}EntityTypeDoc index{% endblock %}

{% block content %}
    <h1>EntityTypeDoc index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Code</th>
                <th>Signo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity_type_doc in entity_type_docs %}
            <tr>
                <td>{{ entity_type_doc.id }}</td>
                <td>{{ entity_type_doc.name }}</td>
                <td>{{ entity_type_doc.code }}</td>
                <td>{{ entity_type_doc.signo }}</td>
                <td>
                    <a href="{{ path('app_entity_type_doc_show', {'id': entity_type_doc.id}) }}">show</a>
                    <a href="{{ path('app_entity_type_doc_edit', {'id': entity_type_doc.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_entity_type_doc_new') }}">Create new</a>
{% endblock %}
